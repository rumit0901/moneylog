<!DOCTYPE html>
<html>
<body>

<h1>Todo List</h1>

<table>
<thead>
    <tr>
        <th>ID</th>
        <th>Task</th>
        <th>Delete</th>
    </tr>
</thead>
<tbody>
    <%data.forEach(element => { %>
    <tr>
        <td><%= element.todoId %></td>
        <td><%= element.todoTask %></td>
        <td></td>
    </tr>
    <% })%>
</tbody>

</table>

<form action="/" method="POST">
    <input type="text" name="todoTask" placeholder="Enter Task">
    <button type="submit">Add</button>
 </form>
</body>
</html>